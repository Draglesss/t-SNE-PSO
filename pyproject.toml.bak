[build-system]
requires = ["setuptools>=61.0", "wheel"]
build-backend = "setuptools.build_meta"

[project]
name = "tsne_pso"
version = "1.1.2"
description = "t-Distributed Stochastic Neighbor Embedding with Particle Swarm Optimization"
readme = "README.md"
requires-python = ">=3.9"
license = "BSD-3-Clause"
authors = [
    {name = "Allaoui, Mebarka", email = "mebarka.allaoui@gmail.com"},
    {name = "Belhaouari, Samir Brahim"},
    {name = "Hedjam, Rachid"},
    {name = "Bouanane, Khadra"},
    {name = "Kherfi, Mohammed Lamine"}
]
maintainers = [
    {name = "Otmane Fatteh", email = "fattehotmane@hotmail.com"}
]
classifiers = [
    "Programming Language :: Python :: 3",
    "Operating System :: OS Independent",
    "Intended Audience :: Science/Research",
    "Topic :: Scientific/Engineering :: Artificial Intelligence",
]
keywords = ["t-SNE", "Particle Swarm Optimization", "Dimensionality Reduction", "Machine Learning"]
dependencies = [
    "numpy>=1.19.5",
    "scipy>=1.6.0",
    "scikit-learn>=1.0.0",
    "umap-learn>=0.5.3",
    "tqdm>=4.64.0",
]

[project.urls]
"Homepage" = "https://github.com/dragless/t-SNE-PSO"
"Bug Tracker" = "https://github.com/dragless/t-SNE-PSO/issues"
"Documentation" = "https://github.com/dragless/t-SNE-PSO#readme"
"Paper" = "https://www.researchgate.net/publication/387835819_t-SNE-PSO_Optimizing_t-SNE_using_particle_swarm_optimization"

